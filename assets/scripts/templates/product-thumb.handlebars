{{#each products as |product|}}
<div class="product-module prod-desc" data-id={{product.id}}>
  <hr>
  {{product.name}} <br>
  <hr>
  ${{product.price}} <br>
  <hr>
  {{product.description}} <br>
  <hr>
  <img class="product-thumb" src={{product.image}} alt="http://imgur.com/qkPReeD">
  <p class="product-name"></p>
</div>
{{/each}}
