{{#each orders as |order|}}
  <div class="order-history-wrapper">
    <p class="order-history-id col-xs-4">{{order._id}}</p>
    <div class="order-history-items col-xs-4">
      {{#each items as |item|}}
      Item name: {{item.name}}<br>
      Item price: ${{item.price}}<br>
      Item count: {{item.count}}<br>
      <img src={{item.image}} alt="watch" width="50px" height="50px"/><br>
      {{/each}}
    </div>
    <p class="order-history-date col-xs-4">Order date: {{order.createdAt}}</p>
  </div>
{{/each}}
